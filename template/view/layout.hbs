<body>

  <header>
    <a href="/" tile="Find rock climbing partners" target="_self"><i></i><span><b>Climb</b>find</span></a>
  </header>

  {{{body}}}

{{#if authenticated}}
    <footer>
      <ul>
        <li><a id="f-home" class="ion-ios-list" href="/" target="_self"></a></li><li><a id="f-inbox" class="ion-chatboxes" href="/messages" target="_self"></a></li><li><a id="f-post" class="ion-compose" href="/partner-call" target="_self"></a></li><li><a id="f-love" class="ion-heart" href="/love" target="_self"></a></li><li><a id="f-account" class="ion-person" href="/account" target="_self"></a></li>
      </ul>
    </footer>
    <script type="text/javascript"> 
      if (document.getElementById('f-{{view}}'))
        document.getElementById('f-{{view}}').className += " active";      

      if (window.location.hash && window.location.hash == '#_=_') {
        if (window.history && history.pushState) {
          window.history.pushState("", document.title, window.location.pathname+window.location.search);
        } else {
          var scroll = { top: document.body.scrollTop, left: document.body.scrollLeft };
          window.location.hash = '';
          document.body.scrollTop = scroll.top;
          document.body.scrollLeft = scroll.left;
        }
      }
    </script>
{{/if}}
